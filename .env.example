# NextAuth Configuration
NEXTAUTH_URL=http://localhost:9002
NEXTAUTH_SECRET=your_random_secret_key_here_generate_with_openssl_rand_base64_32

# Vercel Postgres Database
# This is automatically provided by Vercel when you add Postgres to your project
POSTGRES_URL="postgres://default:password@host/database"
POSTGRES_PRISMA_URL="postgres://default:password@host/database?pgbouncer=true"
POSTGRES_URL_NON_POOLING="postgres://default:password@host/database"
POSTGRES_USER="default"
POSTGRES_HOST="host"
POSTGRES_PASSWORD="password"
POSTGRES_DATABASE="database"

# Google AI (Gemini)
GOOGLE_GENAI_API_KEY=your_gemini_api_key

# Hedera Configuration
HEDERA_NETWORK=testnet
HEDERA_ACCOUNT_ID=0.0.xxxxx
HEDERA_PRIVATE_KEY=your_hedera_private_key
HEDERA_TOKEN_ID=0.0.xxxxx

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:9002
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# Optional: Sentry (Error Tracking)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn
